{
  "name": "leaflet.ajax",
  "version": "1.1.0",
  "description": "AJAX and JSONP in Leaflet",
  "main": "dist/leaflet.ajax.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "npm run jshint && npm run concat && npm run min",
    "concat": "cat src/promise.js src/ajax.js src/jsonp.js src/GeoJSON.js > dist/leaflet.ajax.js",
    "min": "uglifyjs -mc < dist/leaflet.ajax.js > dist/leaflet.ajax.min.js",
    "jshint": "jshint src/ajax.js src/jsonp.js src/GeoJSON.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/calvinmetcalf/leaflet-ajax.git"
  },
  "keywords": [
    "leaflet",
    "ajax",
    "geojson"
  ],
  "author": "Calvin Metcalf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calvinmetcalf/leaflet-ajax/issues"
  },
  "devDependencies": {
    "jshint": "^2.8.0",
    "uglify-js": "^2.4.24"
  }
}
